  "bugs": {
    "url": "https://github.com/gameunits/node-tip-bot/issues"
  },
  "homepage": "https://github.com/gameunits/node-tip-bot",
  "keywords": ["gameunits", "bitcoin", "litecoin", "altcoin", "tip", "tipbot", "irc", "bot"]
}
